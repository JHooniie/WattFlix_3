<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.spring_boot_wattflix.project.model.GameVO;">
  	<resultMap id="gameResult" type="com.spring_boot_wattflix.project.model.GameVO" >
  		<result property="gameNo" column="gameNo"/>
  		<result property="memId" column="memId"/>
  		<result property="movieNo" column="movieNo"/>
  		<result property="myMovie" column="myMovie"/>
  		<result property="myMovieRegdate" column="myMovieRegdate"/>
  	</resultMap>
  	
  	 <!--등록-->
  <insert id="insertGame" parameterType="com.spring_boot_wattflix.project.model.GameVO;">
  	INSERT INTO vsGame(gameNo, memId, movieNo, myMovie, myMovieRegdate)
  	VALUES(#{gameNo}, #{memId}, #{movieNo}, #{myMovie}, #{myMovieRegdate})
  </insert>
  	  </mapper>

  